    "test:e2e:headed": "playwright test --headed",
    "test:smoke": "vitest run --reporter=verbose --grep @smoke",
    "test:regression": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:component && npm run test:e2e",
    "test:watch": "vitest --watch",
    "test:dashboard": "npm run test:ui",
    "test:comprehensive": "node scripts/run-comprehensive-tests.js",
    "test:ci": "npm run test:run -- --reporter=junit --outputFile=test-results.xml && npm run test:e2e -- --reporter=junit --output-dir=test-results"
